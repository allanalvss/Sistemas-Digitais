-- bloco de operacoes 

entity addint is 
port (a,b: in integer range 0 to 255;
		s: out integer range 0 to 255);
end addint;


architecture arq of addint is

begin
s<=a+b; 				--adicao
--s<=a*b;	 		--multiplicacao	
--s<=a-b; 			--subtraÃ§ao
--s<=a/b;			--divisao
end arq;
		
		
